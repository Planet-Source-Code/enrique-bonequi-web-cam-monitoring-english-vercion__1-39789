Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)
Select Case Button.Key
Case "play"
On Error GoTo a
player.Play
a:
Case "stop"
player.Stop
Case "pausa"
On Error GoTo q
player.Pause
q:
Case "salir"
Unload Me
Me.Hide
End Select
End Sub
